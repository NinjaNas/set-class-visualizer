    "format": "prettier --write src/"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "d3-dag": "^1.0.0-ts4",
    "jzz": "^1.6.8",
    "jzz-input-kbd": "^1.2.7",
    "jzz-midi-smf": "^1.7.9",
    "jzz-synth-tiny": "^1.3.6",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.11.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.16.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "typescript": "~5.3.3",
    "vite": "^5.1.6",
    "vue-tsc": "^1.8.8"
